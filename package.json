{
  "name": "hbar-ws",
  "version": "1.0.13",
  "description": "SockJS Abstraction",
  "main": "index.js",
  "scripts": {
    "prebuild" : "babel libs/SocketClient.js --presets es2015 > dist/SocketClient.es5.js",
    "build": "browserify dist/SocketClient.es5.js > dist/SocketClient.js --standalone SocketClient",
    "postbuild": "uglifyjs dist/SocketClient.js -m -c > dist/SocketClient.min.js && rm dist/SocketClient.es5.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hbar-digital/hbar-ws.git"
  },
  "keywords": [
    "websockets",
    "sockets",
    "ws",
    "sockjs"
  ],
  "author": "hbar digital",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hbar-digital/hbar-ws/issues"
  },
  "homepage": "https://github.com/hbar-digital/hbar-ws#readme",
  "dependencies": {
    "sockjs": "^0.3.18",
    "sockjs-client": "^1.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "browserify": "^13.1.0",
    "uglify-js": "^2.7.3"
  }
}
